{
	"name": "pr-reviewer",
	"version": "2.1.3",
	"description": "Local PR review tool that sends AI-processed feedback to MCP-compatible coding agents",
	"author": "Chau Tran <nartc7789@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/nartc/pr-reviewer.git"
	},
	"keywords": [
		"pr-review",
		"code-review",
		"mcp",
		"model-context-protocol",
		"ai",
		"local",
		"git",
		"diff",
		"claude-code",
		"cline"
	],
	"type": "module",
	"scripts": {
		"build": "react-router build && pnpm build:mcp",
		"build:mcp": "tsc -p mcp-server/tsconfig.json",
		"build:cli": "pnpm build && node scripts/bundle-cli-artifacts.js && pnpm --dir cli build",
		"dev": "node scripts/dev.js",
		"dev:app": "cross-env NODE_ENV=development node server.js",
		"mcp": "node dist/mcp-server/index.js",
		"setup:mcp": "node scripts/setup-mcp.js",
		"start": "node server.js",
		"typecheck": "react-router typegen && tsc -b"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^3.0.35",
		"@ai-sdk/google": "^3.0.20",
		"@ai-sdk/openai": "^3.0.25",
		"@effect/platform": "^0.94.2",
		"@effect/platform-node": "^0.104.1",
		"@modelcontextprotocol/sdk": "^1.25.3",
		"@pierre/diffs": "^1.0.10",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@radix-ui/themes": "^3.3.0",
		"@react-router/express": "7.13.0",
		"@react-router/node": "7.13.0",
		"@tanstack/react-virtual": "^3.13.18",
		"ai": "^6.0.68",
		"compression": "^1.8.1",
		"effect": "^3.19.15",
		"express": "^5.2.1",
		"isbot": "^5.1.34",
		"morgan": "^1.10.1",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"react-icons": "^5.5.0",
		"react-router": "7.13.0",
		"remix-utils": "^9.0.1",
		"simple-git": "^3.30.0",
		"sonner": "^2.0.7",
		"sql.js": "^1.12.0"
	},
	"devDependencies": {
		"@react-router/dev": "7.13.0",
		"@tailwindcss/vite": "^4.1.18",
		"@types/compression": "^1.8.1",
		"@types/express": "^5.0.6",
		"@types/express-serve-static-core": "^5.1.1",
		"@types/morgan": "^1.9.10",
		"@types/node": "^24",
		"@types/react": "^19.2.10",
		"@types/react-dom": "^19.2.3",
		"@types/sql.js": "^1.4.9",
		"concurrently": "^9.2.1",
		"cross-env": "^10.1.0",
		"prettier": "^3.8.1",
		"prettier-plugin-organize-imports": "^4.3.0",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vite-tsconfig-paths": "^6.0.5",
		"zod": "^4.3.6"
	},
	"prettier": {
		"singleQuote": true,
		"useTabs": true,
		"tabWidth": 4,
		"plugins": [
			"prettier-plugin-organize-imports"
		]
	},
	"packageManager": "pnpm@10.28.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
